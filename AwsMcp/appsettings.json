{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "None",
      "System": "None",
      "Amazon": "None",
      "AWS": "None"
    }
  },
  "AwsConfiguration": {
    "Region": "us-east-1",
    "TimeoutSeconds": 30,
    "MaxRetryAttempts": 3,
    "UseHttps": true,
    "ForcePathStyle": false
  },
  "AllowedHosts": "*"
}
