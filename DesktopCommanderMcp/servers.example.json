      "name": "Go Analyzer Tools",
      "url": "http://localhost:7300",
      "port": 7300,
      "projectPath": "<Path to project>",
      "initCommands": [
        "go mod tidy",
        "go mod download",
        "go install github.com/swaggo/swag/cmd/swag@latest",
        "swag init -g http_server.go -o ./docs"
      ],
      "startCommand": "go run http_server.go",
      "requiresInit": true
    },
    "typescript-analyzer": {
      "name": "Typescript Analyzer Tools",
      "url": "http://localhost:7302",
      "port": 7302,
      "projectPath": "<Path to project>",
      "initCommands": [
        "npm install",
        "npx tsoa spec-and-routes",
        "npm run build"
      ],
      "startCommand": "node dist/http_server.js",
      "requiresInit": true
    },
    "python-analyzer": {
      "name": "Python Analyzer Tools",
      "url": "http://localhost:7301",
      "port": 7301,
      "projectPath": "<Path to project>",
      "initCommands": [
        "python -m venv .venv",
        ".venv\\Scripts\\pip.exe install -r requirements.txt"
      ],
      "startCommand": ".venv\\Scripts\\python.exe http_server.py",
      "requiresInit": true,
      "initCheckPath": ".venv\\Scripts\\python.exe"
    }
  },
  "usage": "Call GET {url}/description to see available endpoints and capabilities."
